{
  "info": {
    "name": "LinkedIn Analytics Backend API",
    "description": "Complete API collection for LinkedIn Analytics Backend with sample data",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    },
    {
      "key": "access_token",
      "value": "",
      "type": "string"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{access_token}}",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register Admin User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@example.com\",\n  \"username\": \"admin\",\n  \"password\": \"admin123\",\n  \"role\": \"admin\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/register",
              "host": ["{{base_url}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Register Regular User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"username\": \"user1\",\n  \"password\": \"user123\",\n  \"role\": \"user\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/register",
              "host": ["{{base_url}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login Admin",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('access_token', response.access_token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/x-www-form-urlencoded"
              }
            ],
            "body": {
              "mode": "urlencoded",
              "urlencoded": [
                {
                  "key": "username",
                  "value": "admin@example.com"
                },
                {
                  "key": "password",
                  "value": "admin123"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": ["{{base_url}}"],
              "path": ["auth", "login"]
            }
          }
        },
        {
          "name": "Login User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('access_token', response.access_token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/x-www-form-urlencoded"
              }
            ],
            "body": {
              "mode": "urlencoded",
              "urlencoded": [
                {
                  "key": "username",
                  "value": "user@example.com"
                },
                {
                  "key": "password",
                  "value": "user123"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": ["{{base_url}}"],
              "path": ["auth", "login"]
            }
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/auth/me",
              "host": ["{{base_url}}"],
              "path": ["auth", "me"]
            }
          }
        }
      ]
    },
    {
      "name": "Posts",
      "item": [
        {
          "name": "Create Draft Post",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"My First LinkedIn Post\",\n  \"content\": \"This is my first post on LinkedIn! Excited to share my thoughts and connect with professionals in my field.\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/posts/",
              "host": ["{{base_url}}"],
              "path": ["posts", ""]
            }
          }
        },
        {
          "name": "Create Scheduled Post",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Scheduled Post About AI\",\n  \"content\": \"Artificial Intelligence is revolutionizing the way we work. Here are some key trends to watch in 2024.\",\n  \"scheduled_at\": \"2024-12-25T10:00:00Z\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/posts/",
              "host": ["{{base_url}}"],
              "path": ["posts", ""]
            }
          }
        },
        {
          "name": "Get All Posts",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/posts/?limit=10&offset=0",
              "host": ["{{base_url}}"],
              "path": ["posts", ""],
              "query": [
                {
                  "key": "limit",
                  "value": "10"
                },
                {
                  "key": "offset",
                  "value": "0"
                }
              ]
            }
          }
        },
        {
          "name": "Get Posts by Status",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/posts/?status=published&limit=5",
              "host": ["{{base_url}}"],
              "path": ["posts", ""],
              "query": [
                {
                  "key": "status",
                  "value": "published"
                },
                {
                  "key": "limit",
                  "value": "5"
                }
              ]
            }
          }
        },
        {
          "name": "Get Specific Post",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/posts/1",
              "host": ["{{base_url}}"],
              "path": ["posts", "1"]
            }
          }
        },
        {
          "name": "Update Post",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Updated: My First LinkedIn Post\",\n  \"content\": \"This is my updated first post on LinkedIn! I've added some new insights about professional networking.\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/posts/1",
              "host": ["{{base_url}}"],
              "path": ["posts", "1"]
            }
          }
        },
        {
          "name": "Publish Post",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{base_url}}/posts/1/publish",
              "host": ["{{base_url}}"],
              "path": ["posts", "1", "publish"]
            }
          }
        },
        {
          "name": "Delete Post",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/posts/1",
              "host": ["{{base_url}}"],
              "path": ["posts", "1"]
            }
          }
        }
      ]
    },
